{
    "name": "frontend",
    "version": "1.2.0",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "clean": "react-native clear",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "installAll": "npm install && cd ios && pod install",
        "lint-fix": "eslint --ext .ts --ext .tsx --ext .js src --fix",
        "android:development": "ENVFILE=.env.development react-native run-android",
        "android:test": "ENVFILE=.env.test react-native run-android",
        "android:production": "ENVFILE=.env.production react-native run-android",
        "build-android-test": "cd android && ENVFILE=.env.test ./gradlew assembleRelease",
        "build-android-prod": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
        "android-clean": "cd android && ./gradlew clean",
        "bundle-ios": "node node_modules/react-native/local-cli/cli.js bundle --entry-file index.js --platform ios --dev false --bundle-output ./ios/bundle/index.jsbundle --assets-dest ./ios/bundle"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "^1.16.0",
        "@react-native-clipboard/clipboard": "^1.8.5",
        "ahooks": "^3.1.9",
        "axios": "^0.25.0",
        "cnchar": "^3.0.6",
        "cnchar-poly": "^3.0.6",
        "mobx": "6.3.2",
        "mobx-react-lite": "3.1.6",
        "moment": "^2.29.1",
        "pinyin-pro": "^3.8.3",
        "react": "17.0.2",
        "react-native": "0.67.1",
        "react-native-blob-util": "^0.15.0",
        "react-native-bouncy-checkbox": "^2.1.10",
        "react-native-bouncy-checkbox-group": "^0.1.0",
        "react-native-config": "^1.4.5",
        "react-native-elements": "^3.4.2",
        "react-native-gesture-handler": "^1.10.3",
        "react-native-image-picker": "^4.7.3",
        "react-native-modal": "^13.0.0",
        "react-native-pdf": "^6.4.0",
        "react-native-reanimated": "^2.2.4",
        "react-native-render-html": "^6.3.4",
        "react-native-root-toast": "^3.3.0",
        "react-native-safe-area-context": "^3.4.0",
        "react-native-screens": "^3.9.0",
        "react-native-splash-screen": "^3.3.0",
        "react-native-svg": "^12.1.1",
        "react-native-swiper": "^1.6.0",
        "react-native-vector-icons": "^9.1.0",
        "react-native-webview": "^11.18.1",
        "react-navigation-tabs": "^2.11.1",
        "validator": "^13.7.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "@react-navigation/bottom-tabs": "^6.0.9",
        "@react-navigation/drawer": "^6.1.8",
        "@react-navigation/native": "^6.0.6",
        "@react-navigation/native-stack": "^6.2.5",
        "@react-navigation/stack": "^6.0.11",
        "@types/lodash": "^4.14.177",
        "@types/react-native-vector-icons": "^6.4.10",
        "@types/validator": "^13.7.1",
        "babel-jest": "^26.6.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "7.14.0",
        "jest": "^26.6.3",
        "metro-react-native-babel-preset": "^0.66.2",
        "react-test-renderer": "17.0.2",
        "typescript": "^4.5.2"
    },
    "jest": {
        "preset": "react-native"
    }
}
